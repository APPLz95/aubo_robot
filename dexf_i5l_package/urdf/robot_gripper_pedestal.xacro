<?xml version="1.0"?>
<robot  xmlns:xacro="http://ros.org/wiki/xacro" name="i5l">

<xacro:include filename="$(find dexf_i5l_package)/urdf/aubo_i5L.urdf.xacro" />


<!-- start of arm-vacuum gripper coupling link definitions-->
  <link name="vacuum_gripper">
    <inertial> 
      <origin xyz="0 0 0" rpy="0 0 0"/> <mass value="0.8"/> <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="package://dexf_i5l_package/meshesL/gripper/7_dof_vacuum_no_camera.stl" />
      </geometry>
      <material name="metal">
        <color rgba="0.5 0.5 0.6 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://dexf_i5l_package/meshesL/gripper/7_dof_vacuum_no_camera(collision).dae" />
      </geometry>
       <material name="metal">
        <color rgba="0.5 0.5 0.6 1"/>
      </material>
    </collision>
  </link>

<!-- end of arm-vacuum gripper coupling link definitions --> 

  <!-- start of arm-gripper coupling joint definitions -->
  <joint name="wrist_to_gripper" type="fixed">
    <parent link="ee_link"/>
    <child link="vacuum_gripper"/>
    <origin xyz="0.0 0.0 0.0" rpy="3.14 1.57 0"/>
  </joint>

  <link name="vacuum_gripper_eef_link">
    <inertial> 
      <origin xyz="0 0 0" rpy="0 0 0"/> <mass value="0.1"/> <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
      <geometry>
        <!-- Uncomment Below section for Old Suction cup -->
        <!--<mesh filename="package://dexf_i5l_package/meshesL/gripper/vacuum/Part3_New.stl" />-->
        <mesh filename="package://dexf_i5l_package/meshesL/gripper/New_Suction_35mm.stl" />
      </geometry>
      <material name="metal">
        <color rgba="0.5 0.5 0.6 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <!-- Uncomment Below section for Old Suction cup -->
        <!--<mesh filename="package://dexf_i5l_package/meshesL/gripper/vacuum/Part3_Collision.dae" />-->
        <mesh filename="package://dexf_i5l_package/meshesL/gripper/New_Suction_35mm_Collision.dae" />
      </geometry>
       <material name="metal">
        <color rgba="0.5 0.5 0.6 1"/>
      </material>
    </collision>
  </link>

    <!-- start of arm-gripper coupling joint definitions -->
  <joint name="gripper_to_eef" type="revolute">
    <parent link="vacuum_gripper"/>
    <child link="vacuum_gripper_eef_link"/>
    <!-- Uncomment Below section for Old Suction cup -->
    <!--<origin xyz="-0.2025 0.010 0.045" rpy="0.0 0.0 0.0" />-->
    <origin xyz="-0.2025 0.0 0.025" rpy="0.0 0.0 0.0" />
      <axis xyz="0 0 1" />
      <!-- Motor position limit from 0 to 300 degrees and max velocity 116 RPM -->
      <limit lower="-2.5" upper="2.5" effort="28.0" velocity="12.14"/>
      <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <transmission name="gripper_eef_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="gripper_to_eef">
      <hardwareInterface>PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="gripper_eef_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>


  <!-- start of arm-pedestal coupling link definitions-->
  <link name="pedestal_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <pose>0 0 0 0 0 0</pose>
          <velocity>0 0 0 0 0 0</velocity>
          <acceleration>0 0 0 0 -0 0</acceleration>
          <wrench>0 0 0 0 -0 0</wrench>
      <geometry>
        <mesh filename="package://dexf_i5l_package/meshesL/pedestal/aubo_stand.stl" />
      </geometry>
      <material name="metal">
        <color rgba="0.5 0.5 0.6 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <pose>0 0 0 0 0 0</pose>
          <velocity>0 0 0 0 0 0</velocity>
          <acceleration>0 0 0 0 -0 0</acceleration>
          <wrench>0 0 0 0 -0 0</wrench>
      <geometry>
        <mesh filename="package://dexf_i5l_package/meshesL/pedestal/aubo_stand.stl" />
      </geometry>
       <material name="metal">
        <color rgba="0.5 0.5 0.6 1"/>
      </material>
    </collision>
  </link>


<!-- end of arm-pedestal coupling link definitions --> 

  <!-- start of arm-pedestal coupling joint definitions -->
  <joint name="pedestal" type="fixed">
    <parent link="pedestal_link"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.43004" rpy="0 0 0"/>
  </joint>

  <link name="world" />

  <joint name="world_joint" type="fixed">
    <parent link="world" />
    <child link = "pedestal_link" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>


</robot>  